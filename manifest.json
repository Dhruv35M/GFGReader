{
  "manifest_version": 3,
  "name": "GFGReader",
  "version": "0.0.1",
  "description": "Distraction-Free Reading for Geeksforgeeks",
  "content_scripts": [
    {
      "matches": ["https://*.geeksforgeeks.org/*"],
      "css": ["styles.css"]
    }
  ],
  "author": "Dhruv Mishra",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
